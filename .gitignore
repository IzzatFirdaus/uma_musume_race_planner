# Uma Musume Race Planner - cleaned .gitignore

# Environment and local secrets
.env
.env.local
includes/env.php

# Logs and runtime
/logs/
*.log

# Uploads: ignore runtime uploads but keep app logo assets tracked
/uploads/
!uploads/app_logo/
!uploads/app_logo/*.png
!uploads/app_logo/*.ico

# Temporary and cache
/tmp/
tmp/
*.tmp
*.bak
*.swp

# Node
node_modules/
npm-debug.log*
package-lock.json
yarn.lock

# Build / Dist
/dist/
/build/

# Composer dependencies (optional - if you vendor dependencies, uncomment)
# /vendor/
# Keep composer.lock tracked

# OS / Editor files
.DS_Store
Thumbs.db
.vscode/
.idea/
*.sublime-project
*.sublime-workspace
*.iml

# Test / Coverage
/coverage/
phpunit.result.cache
phpunit.xml
phpunit.xml.dist
phpunit.xml.cache

# Database dumps / backups
*.sql
*.sql.gz
*.backup
backup/

# Generated assets
*.min.js
*.bundle.js
*.map

# Archives
*.zip
*.tar.gz
*.7z

# Composer dependencies (vendor directory should generally be ignored when using composer install)
/vendor/

